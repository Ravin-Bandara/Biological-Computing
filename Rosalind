s = Seq(""
       )
t = Seq("TCACTCATC"
       )


locations = []
count = 0
loc = 0

while (count < len(s)):
    loc = s.find(t, start=count)
    if (loc>0):
        count = loc + 1
        locations.append(loc+1)
    else: 
        count = count + 1
        
print(' '.join(map(str,locations)))

print("\n---\n")
